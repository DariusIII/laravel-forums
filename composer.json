{
    "name": "dariusiii/laravel-forums",
    "description": "Forums and discussion boards for Laravel, using the Vanilla Forums engine.",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Bishop Bettini", "email": "bishop@php.net"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vanilla/vanilla"
        }
    ],
    "require": {
        "vanilla/vanilla": "dev-master",
        "laravel/framework": "^5.8",
        "watson/validating": "^3.2.0",
        "felixkiss/uniquewith-validator": "^3.2.0"
    },
    "autoload": {
        "classmap": [ "src" ],
        "psr-4": {
            "DariusIII\\Forum\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.2",
        "phpunit/phpunit": "^8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "DariusIII\\ForumTest\\": "tests/"
        }
    }
}
